<?xml version="1.0"?>
<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 6.2.0//EN" "http://www.liferay.com/dtd/liferay-portlet-app_6_2_0.dtd">

<liferay-portlet-app>
    <portlet>
        <portlet-name>semantic-query</portlet-name>
        <icon>/icon.png</icon>
        <header-portal-css>/js/semantic_query/app.css</header-portal-css>
        <header-portal-css>/js/semantic_query/bower_components/ol.css</header-portal-css>
        <header-portlet-css>/css/main.css</header-portlet-css>
        <footer-portal-javascript>http://cdnjs.cloudflare.com/ajax/libs/jquery-cookie/1.4.1/jquery.cookie.js</footer-portal-javascript>
        <footer-portal-javascript>/js/semantic_query/bower_components/ol.js</footer-portal-javascript>
        <footer-portal-javascript>/js/semantic_query/index.js</footer-portal-javascript>
    </portlet>
    <role-mapper>
        <role-name>administrator</role-name>
        <role-link>Administrator</role-link>
    </role-mapper>
    <role-mapper>
        <role-name>guest</role-name>
        <role-link>Guest</role-link>
    </role-mapper>
    <role-mapper>
        <role-name>power-user</role-name>
        <role-link>Power User</role-link>
    </role-mapper>
    <role-mapper>
        <role-name>user</role-name>
        <role-link>User</role-link>
    </role-mapper>
</liferay-portlet-app>
