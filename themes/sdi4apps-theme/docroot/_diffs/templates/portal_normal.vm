<!DOCTYPE html>

#parse ($init)

<html class="$root_css_class" dir="#language ("lang.dir")" lang="$w3c_language_id">

<head>
    <title>$the_title - $company_name</title>

    <meta content="initial-scale=1.0, width=device-width" name="viewport" />
    <script src="/wwwlibs/hslayers/HS.js" type="text/javascript"></script>

    <script type="text/javascript">
        var getLRlang = function() {
            return "$themeDisplay.getLocale().getLanguage()";
        }

        var isPOI = function () {
        #set ( $usrPoi = false )
        #if ($is_signed_in)
            #set ( $usrRoles = $user.getRoles())
            #foreach ( $usrRole in $usrRoles )
                #if ( $usrRole.getName() == "poi" )
                    #set ( $usrPoi = true )
                #end
            #end
        #end
        return $usrPoi
        }

        var getLRUser = function() {
        #if ($is_signed_in)
        return "$user.getScreenName()";
        #else
        return "guest";
        #end
        }
    </script>

    #set ($widthtype = $theme.getSetting("content-full-width"))
    #set ($footer = $theme.getSetting("footer"))

    $theme.include($top_head_include)
    <link rel="stylesheet" media="screen,projection" type="text/css" href="/wwwlibs/ext/resources/css/ext-all.css"></link>
    <link rel="stylesheet" media="screen,projection" type="text/css" href="/wwwlibs/ext/resources/css/xtheme-gray.css"></link>
    <link rel='stylesheet' id='googleFonts-css'  href='//fonts.googleapis.com/css?family=Oxygen:400,300,700' type='text/css' media='all' />

    <script src="$javascript_folder/iframefull.js" defer="defer"></script>
</head>

<body class="$css_class">

<a href="#main-content" id="skip-to-content">#language ("skip-to-content")</a>

$theme.include($body_top_include)

#if ($is_signed_in)
    #dockbar()
#else
    #set($companyLocalService = $serviceLocator.findService("com.liferay.portal.service.CompanyLocalService"))
    #set($company = $companyLocalService.getCompany($company_id))
    #set($portalURL = $company.getVirtualHostname())
    #set ($create_account_url = $portletURLFactory.create($request, "58", $page.getPlid(), "ACTION_PHASE"))
    $create_account_url.setWindowState("maximized")
    $create_account_url.setPortletMode("view")
    $create_account_url.setParameter("saveLastPath", "0")
    $create_account_url.setParameter("struts_action", "/login/create_account")


<div class="portlet-boundary portlet-boundary_145_ portlet-static portlet-static-end portlet-borderless portlet-dockbar " id="p_p_id_145_">
    <div class="navbar navbar-static-top dockbar" id="_145_dockbar" data-namespace="_145_">
        <div class="navbar-inner">
            <div class="container">
                <div class="collapse nav-collapse" id="_145_navAccountControlsNavbarCollapse">
                    <ul aria-label="Dockbar" class="nav nav-account-controls" id="_145_navAccountControls" role="menubar">
                        <li class="register" role="presentation">
                            <a class="" data-redirect="true" href="$create_account_url" id="" role="menuitem" title="">
                                <span class="nav-item-label"> $languageUtil.get($locale, "Register") </span>
                            </a>
                        </li>
                        <li class="divider-vertical " id="whxm_" role="presentation"> </li>
                        <li class="sign-in " id="ljla_" role="presentation">
                            <a class="" data-redirect="true" href="https://$portalURL/cas-server/login?service=http://$portalURL/php/cas.html" id="" role="menuitem" title="">
                                <i class="icon-user"></i>
                                <span class="nav-item-label"> $sign_in_text </span>
                            </a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <script type="text/javascript">AUI().use("liferay-dockbar",function(a){(function(){Liferay.Dockbar.init("#_145_dockbar");var b=a.all(".portlet-column-content.customizable");if(b.size()>0){b.get("parentNode").addClass("customizable")}})()});</script>
</div>
#end

#parse ("$full_templates_path/header.vm")

#if ($widthtype == true)
<div class="container-fluid no-border" id="wrapper">
#else
<div class="container-fluid" id="wrapper">
#end

    <div id="content">
        #if ($selectable)
            $theme.include($content_include)
        #else
            $portletDisplay.recycle()

            $portletDisplay.setTitle($the_title)

            $theme.wrapPortlet("portlet.vm", $content_include)
        #end
    </div>

    #if ($footer == true)
    #parse ("$full_templates_path/footer.vm")
    #end
</div>

$theme.include($body_bottom_include)

$theme.include($bottom_include)

<script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
<script>window.jQuery || document.write('<script src="$javascript_folder/vendor/jquery-1.11.3.min.js"><\/script>')</script>
</body>

</html>
